   "source": [
    "def cross_entropy(list_Y,list_P):\n",
    "    list_Y = np.float_(list_Y)\n",
    "    list_P = np.float_(list_P)\n",
    "    return -np.sum(list_Y*np.log(list_P)+(1-list_Y)*np.log(1-list_P))"
   ]
